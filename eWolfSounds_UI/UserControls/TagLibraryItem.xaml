<UserControl x:Class="eWolfSounds_UI.UserControls.TagLibraryItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:eWolfSounds_UI.UserControls"
             mc:Ignorable="d"
             d:DesignHeight="32" d:DesignWidth="200">
    <Grid>
        <Label Content="{Binding TagName}" HorizontalAlignment="Center" VerticalAlignment="Center" Width="192" Height="30" MouseDoubleClick="Label_MouseDoubleClick" />
    </Grid>
</UserControl>